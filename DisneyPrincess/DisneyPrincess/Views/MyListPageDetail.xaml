<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DisneyPrincess.Views.MyListPageDetail">
    <ContentPage.Content>
        <StackLayout HorizontalOptions="Center" BackgroundColor="#D2487A">
            <Label Text="Princesas Disney App" FontSize="Large" HorizontalTextAlignment="Center"/>
            <Image x:Name="MyImageCall" HeightRequest="150" WidthRequest="150"/>
            <Label x:Name="MyItemNameShow" HorizontalTextAlignment="Center" FontSize="Large"/>
            <Label x:Name="MyPersonalityItemShow" Margin="20,20" FontSize="Body"/>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>